{
  "listen": {
    "ip": "127.0.0.1",
    "port": 919
  },
  "pidfile": "%{flight_ROOT}/var/run/console-api.pid",
  "sso": {
    "shared_secret_path": "%{flight_ROOT}/etc/shared-secret.conf",
    "cookie_name": "flight_login"
  },
  "ssh": {
    "private_key_path": "%{flight_ROOT}/etc/console/id_rsa",
    "host": null,
    "port": 22,
    "localAddress": null,
    "localPort": null,
    "term": "xterm-color",
    "readyTimeout": 20000,
    "keepaliveInterval": 120000,
    "keepaliveCountMax": 10,
    "allowedSubnets": ["127.0.0.0/8"]
  },
  "session": {
    "name": "flight-console-api",
    "secret": ""
  },
  "options": {
  },
  "algorithms": {
    "kex": [
      "ecdh-sha2-nistp256",
      "ecdh-sha2-nistp384",
      "ecdh-sha2-nistp521",
      "diffie-hellman-group-exchange-sha256",
      "diffie-hellman-group14-sha1"
    ],
    "cipher": [
      "aes128-ctr",
      "aes192-ctr",
      "aes256-ctr",
      "aes128-gcm",
      "aes128-gcm@openssh.com",
      "aes256-gcm",
      "aes256-gcm@openssh.com",
      "aes256-cbc"
    ],
    "hmac": [
      "hmac-sha2-256",
      "hmac-sha2-512",
      "hmac-sha1"
    ],
    "compress": [
      "none",
      "zlib@openssh.com",
      "zlib"
    ]
  },
  "accesslog": true,
  "safeShutdownDuration": 30
}
